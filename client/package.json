{
  "name": "client",
  "version": "0.0.0",
  "config": {
    "deployUrl": "",
    "baseUrl": "./"
  },
  "scripts": {
    "ng": "ng",
    "builddoc": "compodoc -p src/tsconfig.app.json",
    "servedoc": "compodoc -s",
    "buildservedoc": "compodoc -p src/tsconfig.app.json -s --hideGenerator -n 'GMS Angular Client Documentation'",
    "start": "ng serve --watch=true",
    "build": "ng build --prod --deploy-url $npm_package_config_deployUrl --base-href $npm_package_config_baseUrl",
    "test": "ng test",
    "testwatch": "ng test --watch=true",
    "testwatchnosourcemap": "ng test --watch=true --sourceMap=false",
    "testcoverage": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@ng-bootstrap/ng-bootstrap": "2.1.1",
    "@ngx-pwa/local-storage": "^7.0.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.5.7",
    "ng-http-loader": "^2.2.0",
    "ngx-cookie": "^4.0.2",
    "ngx-toastr": "^9.1.1",
    "rxjs": "^6.5.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "8.0.1",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@compodoc/compodoc": "^1.1.5",
    "@compodoc/ngd-cli": "^2.0.0",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.12.0",
    "codelyzer": "~4.5.0",
    "coveralls": "^3.0.2",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "node-sass": "^4.9.4",
    "protractor": "^5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3"
  }
}
